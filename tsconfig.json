{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "src/",
    "paths": {
      "@api/*": ["api/*"],
      "@hook/*": ["hook/*"],
      "@redux/*": ["redux/*"],
      "@style/*": ["style/*"],
      "@screen/*": ["screen/*"],
      "@context/*": ["context/*"],
      "@component/*": ["component/*"],
      "@navigation/*": ["navigation/*"]
    }
  }
}
